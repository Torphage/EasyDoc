{
    "name": "easydoc",
    "displayName": "EasyDoc",
    "description": "Makes custom formats of documentations of functions, classes, etc. for different languages extremely easy",
    "version": "0.1.0",
    "publisher": "Torphage",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/torphage/easydoc"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.EasyDoc"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.EasyDoc",
                "title": "EasyDoc"
            }
        ],
        "configuration": {
            "title": "EasyDoc configurations",
            "properties": {
                "EasyDoc.tomdoc": {
                    "type": "object",
                    "default": {
                        "triggerString": "###",
                        "docType": "function",
                        "commentAboveTarget": false
                    }
                },
                "EasyDoc.dir": {
                    "type": "array",
                    "default": [
                        "./templates"
                    ]
                }
            }
        }
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typedoc": "^0.13.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "tslint": "^5.11.0"
    }
}
