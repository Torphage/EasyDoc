{
    "name": "easydoc",
    "displayName": "EasyDoc",
    "description": "Makes custom formats of documentations of functions, classes, etc. for different languages extremely easy",
    "version": "0.2.1",
    "publisher": "Torphage",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/torphage/easydoc"
    },
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.EasyDoc"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings" : [
            {
                "command": "extension.EasyDoc",
                "key": "ctrl+shift+alt+z",
                "max": "cmd+shift+alt+z"
            }
        ],
        "commands": [
            {
                "command": "extension.EasyDoc",
                "title": "EasyDoc"
            }
        ],
        "configuration": {
            "title": "EasyDoc configurations",
            "properties": {
                "EasyDoc.dir": {
                    "type": "array",
                    "default": [
                        "./templates"
                    ]
                },
                "EasyDoc.tomdoc": {
                    "type": "object",
                    "default": {
                        "triggerString": "EasyDoc_tomdoc",
                        "docType": "function",
                        "commentAboveTarget": false
                    }
                },
                "EasyDoc.a": {
                    "type": "object",
                    "default": {
                        "commentAboveTarget": false,
                        "docType": "function",
                        "triggerString": "EasyDoc_a"
                    }
                },
                "EasyDoc.b": {
                    "type": "object",
                    "default": {
                        "commentAboveTarget": false,
                        "docType": "block",
                        "triggerString": "EasyDoc_b"
                    }
                }
            }
        }
    },
    "scripts": {
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "unit_test": "mocha -r ts-node/register 'test/**/*.spec.ts'"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typedoc": "^0.13.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "tslint": "^5.11.0"
    }
}
